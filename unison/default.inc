# or unison will just hang.
log = true
logfile = /home/douglas/.unison/unison.log

ignore = Name {.*~,*~,.#*}
ignore = Name {,.}*{.swp}
ignore = Name {Thumbs.db}
ignore = Name {,.}*{.pyc}
ignore = Name desktop.ini

